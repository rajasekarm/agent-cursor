{
  "name": "agent-cursor",
  "version": "1.0.0",
  "description": "A programmatic cursor control library for AI agents and browser automation to interact with web pages",
  "type": "module",
  "main": "./lib/agent-cursor.js",
  "types": "./lib/agent-cursor.d.ts",
  "exports": {
    ".": {
      "import": "./lib/agent-cursor.js",
      "types": "./lib/agent-cursor.d.ts"
    }
  },
  "files": [
    "lib",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc",
    "build:demo": "tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --write .",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:report": "playwright show-report",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "cursor",
    "agent",
    "automation",
    "browser-automation",
    "web-automation",
    "ai-agent",
    "browser",
    "typescript",
    "ui-automation",
    "programmatic-control",
    "form-automation",
    "cursor-control"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rajasekarm/agent-cursor.git"
  },
  "bugs": {
    "url": "https://github.com/rajasekarm/agent-cursor/issues"
  },
  "homepage": "https://github.com/rajasekarm/agent-cursor#readme",
  "author": "Rajasekarm",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@types/node": "^24.6.2",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.11"
  }
}
